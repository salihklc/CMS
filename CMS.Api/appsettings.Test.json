{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ConnectionStrings": {
    "CmsDatabase": "Server=tcp:localhost,1433;Database=Cms;User Id=cms;Password=Cms.123;"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "Server=tcp:localhost,1433;Database=CmsLog;User Id=cms;Password=Cms.123;",
          "tableName": "Logs",
          "autoCreateSqlTable": true,
          "columnOptionsSection": {
            "primaryKeyColumnName": "Idx",
            "addStandardColumns": [
              "LogEvent"
            ],
            "id": {
              "columnName": "Idx",
              "nonClusteredIndex": true
            },
            "timeStamp": {
              "columnName": "InsertDate",
              "AllowNull": false
            },
            "messageTemplate": { "columnName": "Template" },
            "customColumns": [
              {
                "ColumnName": "UniqueId",
                "DataType": "nvarchar",
                "DataLength": 50,
                "AllowNull": true
              },
              {
                "ColumnName": "UserName",
                "DataType": "nvarchar",
                "DataLength": 256,
                "AllowNull": true
              },
              {
                "ColumnName": "Application",
                "DataType": "nvarchar",
                "DataLength": 256,
                "AllowNull": true
              },
              {
                "ColumnName": "InsertUserIdx",
                "DataType": "int",
                "DataLength": 50,
                "AllowNull": false
              },
              {
                "ColumnName": "UpdateUserIdx",
                "DataType": "int",
                "DataLength": 50,
                "AllowNull": true
              },
              {
                "ColumnName": "UpdateDate",
                "DataType": "datetime",
                "AllowNull": true
              },
              {
                "ColumnName": "Status",
                "DataType": "int"
              }
            ]
          }
        }
      }
    ]
  },
  "IntegratorSettings": {
    "GmailCredential": {
      "ApplicationName": "CmsGmail",
      "CredentialApplicationEmail": "cms@mycms.iam.gserviceaccount.com",
      "CredentialUser": "admin@mycmsdomain.com",
      "CredentialPath": "/var/www/CMS/mygmailsecret.p12"
    },
    "OutlookCredential": {}
  },
  "Folders": {
    "Uploads": "Uploads//",
    "Example": "Examples//",
    "Attachments": "/Cms/FileStorage/Attachments"
  }
}