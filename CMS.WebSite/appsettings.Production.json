{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ConnectionStrings": {
    "CmsDatabase": "*****",
    "cms-log;Database": "*****"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "*****",
          "tableName": "Logs",
          "autoCreateSqlTable": true,
          "columnOptionsSection": {
            "primaryKeyColumnName": "Idx",
            "addStandardColumns": [
              "LogEvent"
            ],
            "id": {
              "columnName": "Idx",
              "nonClusteredIndex": true
            },
            "timeStamp": {
              "columnName": "InsertDate",
              "AllowNull": false
            },
            "messageTemplate": {
              "columnName": "Template"
            },
            "customColumns": [
              {
                "ColumnName": "UniqueId",
                "DataType": "nvarchar",
                "DataLength": 50,
                "AllowNull": true
              },
              {
                "ColumnName": "UserName",
                "DataType": "nvarchar",
                "DataLength": 256,
                "AllowNull": true
              },
              {
                "ColumnName": "Application",
                "DataType": "nvarchar",
                "DataLength": 256,
                "AllowNull": true
              },
              {
                "ColumnName": "InsertUserIdx",
                "DataType": "int",
                "DataLength": 50,
                "AllowNull": false
              },
              {
                "ColumnName": "UpdateUserIdx",
                "DataType": "int",
                "DataLength": 50,
                "AllowNull": true
              },
              {
                "ColumnName": "UpdateDate",
                "DataType": "datetime",
                "AllowNull": true
              },
              {
                "ColumnName": "Status",
                "DataType": "int"
              }
            ]
          }
        }
      }
    ]
  },
  "IntegratorSettings": {
    "GmailCredential": {
      "ApplicationName": "CmsGmail",
      "CredentialApplicationEmail": "support@mycmsservice.iam.gserviceaccount.com",
      "CredentialUser": "talep@CMS.com.tr",
      "CredentialPath": "\\secret\\location\\cms-test.p12"
    },
    "OutlookCredential": {}
  },
  "Folders": {
    "Uploads": "\\Cms\\FileStorage\\PersonalUploads",
    "Documents": "\\Cms\\FileStorage\\OtherDocuments",
    "Attachments": "\\Cms\\FileStorage\\TicketAttachments"
  }
}