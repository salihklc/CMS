    <script type="text/javascript">
        var logoutUrl = '@Url.Action("Logout","Account")';
        function CreateTicketModal() {
            debugger;
            $.ajax({
                type: 'GET',
                url: "@Url.Action("Create","Tickets")",
                success: function (resultData) {
                    debugger;
                    $('#modalBody').html(resultData);
                    $('#myModal').modal();
                    runwysihtml5();
                    runSelect2inModal();
                }
            });
        }
    </script>
