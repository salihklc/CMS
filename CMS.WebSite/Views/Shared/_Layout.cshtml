            <ol class="breadcrumb">
                <li><a href="#"><i class="fa fa-dashboard"></i> Home</a></li>
                <li >Dashboard</li>
              </ol>
            </section> -->
            <!-- Main content -->
            <section class="content">
                @RenderBody()
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->

        @{Html.RenderPartial("~/Views/Shared/Components/Modal/Modal.cshtml");}

        @{Html.RenderPartial("~/Views/Shared/_Footer.cshtml");}

    </div>

    <script>
      var activeMenuSelecter = '@ViewData["SelectedMenu"]';
      var baseAdminPageUrl = "@Url.Content("~")";
    </script>

